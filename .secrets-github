#!/bin/sh
#
# Project development environment / Secrets
#
# * Use to map GitHub CICD variables into project secrets
# * This file is included in the repository, do not include secret values
# * Use the file dot-secrets as reference
#

CNTBUILD_LOGIN_PASSWORD="$GITHUB_TOKEN"
export CNTBUILD_LOGIN_PASSWORD

CNTBUILD_LOGIN_USER="$GITHUB_ACTOR"
export CNTBUILD_LOGIN_USER
